<!DOCTYPE html>
<html>
<head>
  <title>Surveyor</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <%= link_to "profile", user_path(current_user) %>
  <% end %>
  <%= link_to "home", root_path %>
  <%= render "sessions/user_sessions" %>

  <div class='container'>
<%= yield %>
  </div>
</body>
</html>
